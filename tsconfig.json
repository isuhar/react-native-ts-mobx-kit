{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "experimentalDecorators": true,
    "baseUrl": ".",
    "strictPropertyInitialization": false,
    "paths": {
      "src": ["./src"]
    }
  },
  "exclude": ["node_modules"]
}
